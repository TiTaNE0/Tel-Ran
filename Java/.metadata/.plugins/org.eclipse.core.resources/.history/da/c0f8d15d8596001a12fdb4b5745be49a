package telran.employee.controller;

public class EmployeeAppl {

	public static void main(String[] args) {
		Employee peter;
		peter = new Employee();
		peter.id = 123456789;
		peter.name = "Peter";
		peter.salary = 15000;
		peter.taxAllowance = 2.25;
		peter.display();
		Employee john = new Employee();
		john.id = 987654321;
		john.name = "John";
		john.salary = 17000;
		john.taxAllowance = 2.25;
		john.display();
		double tax = peter.taxIsrael();
		System.out.println(tax);
		

	}
}	
		
