#include <stdio.h>

void activateHydrogen();
void engineStart();
void boostersIgnite();
void boostersRelease();
void liftOff();

int main(){
	printf("T-10 seconds counting to blast off:\n");
	liftOff();
	return 0;
}

void liftOff(){
	printf("Explosive bolts release the boosters...")
	boostersRelease();
	printf("-= 10 =-");
}

void boostersRelease(){
	printf("Solid Rocket Boosters ignite...");
	boostersIgnite();
	printf("-= 9 =-");

}

void boostersIgnite(){
	printf("Main engine start...");
	engineStart();
	printf("-= 8 =-")
}

void engineStart(){
	printf("Activate main engine hydrogen burnoff system...");
}

