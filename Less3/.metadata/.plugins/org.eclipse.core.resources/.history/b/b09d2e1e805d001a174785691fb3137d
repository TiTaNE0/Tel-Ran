#include<stdio.h>

int main(){
	lightOn();
	entertainer();
	lightOff;
	return 0;
}

void lightOn(){
	printf("light on\n");
}

void lightOff(){
	printf("light off\n")
}

void entertainer(){
	juggler();
	singer();
	clown();

}

void juggler(){
	printf("I am a juggler\n");
	printf("My name John Smith\n");
}
