void boostersRelease();
void liftOff();

int main(){
	printf("T-10 seconds counting to blast off:\n");
	printf("Activate main engine hydrogen burnoff system...\n");
	activateHydrogen();
	return 0;
}

void activateHydrogen(){
	printf("Explosive bolts release the boosters...\n");
	engineStart();
	printf("-= 10 =-");
}

void boostersIgnite(){
	printf("Solid Rocket Boosters ignite...\n");
	boostersRelease();
	printf("-= 9 =-");

}

void boostersIgnite(){
	printf("Main engine start...\n");
	engineStart();
	printf("-= 8 =-");
}

void engineStart(){
	printf("Activate main engine hydrogen burnoff system...\n");
}

