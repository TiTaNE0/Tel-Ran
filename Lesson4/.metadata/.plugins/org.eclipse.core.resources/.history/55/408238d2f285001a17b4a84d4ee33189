#include<stdio.h>

void printArr(int arr[], int size);


int main () {


	return 0;
}


void printArr(int arr[], int size) {
	int i;
	printf("{ ");
	for (i = 0; i < size; i++) {
		printf("%d ", arr[i]);
	}
	printf(" }\n");
