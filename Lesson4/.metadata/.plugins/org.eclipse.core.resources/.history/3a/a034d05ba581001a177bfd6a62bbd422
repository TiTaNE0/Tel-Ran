#include<stdio.h>
void starPrinterX(int q);

int main() {

	starPrinterX(11);

	return 0;
}

void starPrinterX(int q) {
	int i, j;
	for (i = 1; i <= q; i++) {
		for (j = 1; j <= q; j++) {
			if (i == 1 ||  i == q || j == 1 || j == q || i == j || j == q-i+1
				|| j == i/2-2){
				printf("* ");
			} else {
			printf("  ");
			}
		}
		printf("\n");
	}
}




