#include<stdio.h>
#include<stdlib.h>
#include<time.h>


void printArr(int arr[], int size);
void fillArr(int arr[], int size, int min, int max);

int main () {

	printf("%d\n", RAND_MAX);

	int size = 10;
	int arr[size];

	fillArr(arr, size, 10, 99);

	printArr(arr, size);

	return 0;
}


void fillArr(int arr[], int size, int min, int max) {
	srand(time(NULL));
	int i;
	for (i = 0; i < size; i++) {
		arr[i] = min rand() % (max + 1);
	}
}


void printArr(int arr[], int size) {
	int i;
	printf("{ ");
	for (i = 0; i < size; i++) {
		printf("%d ", arr[i]);
	}
	printf(" }\n");
}
