#include <stdio.h>

int invertNum(int num);
void contNum(int num, int num1);

int main() {

	int inv = invertNum(123456789);
	printf("%d\n", inv);

	contNum(253705, 37);

}



int invertNum(int num){
	int res = 0;
	int count;
	for (count = 1; num; num /= 10) {
		res *= 10;
		res += num % 10;
	}
	return res;
}
