#include<stdio.h>
void starPrinterX(int q);

int main() {

	starPrinterX(10);

	return 0;
}

void starPrinterX(int q) {
	int i, j;
	for (i = 1; i <= q; i++) {
		for (j = 1; j <= q; j++) {
			if (i == 1 || i == q || j == 1 || j == q || i == j || j == q-i+1 ||
			i == 6) {
				printf("* ");
			} else {
				printf("  ");
			}
//			if (i == 1 || i == q || j == 1 || j == q || i == j || j == q-i+1) {
//				printf("* ");
//			} else {
//				printf("  ");
//			}
		}
		printf("\n");
	}
}




